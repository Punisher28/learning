<T3DataStructure>
	<sheets>
		<!--
					################################
					  SHEET Page Conf
					################################
				-->
		<pageconf>
			<ROOT>
				<TCEforms>
					<sheetTitle>Page Conf</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<!-- PID Detail Page -->
					<settings.detailPid>
						<TCEforms>
							<label>detailPid</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
							</config>
						</TCEforms>
					</settings.detailPid>

					<!-- PID List Page -->
					<settings.listPid>
						<TCEforms>
							<label>listPid</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
							</config>
						</TCEforms>
					</settings.listPid>
				</el>
			</ROOT>
		</pageconf>
	</sheets>
</T3DataStructure>
